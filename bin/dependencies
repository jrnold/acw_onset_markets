#!/usr/bin/env Rscript
args <- commandArgs(TRUE)
outfile <- file(args[1], 'w')

R_DIR <- 'R'
RDATA_DIR <- 'rdata'
R_PATTERN <- '[^.].*\\.R$'

rdata_key_path <- function(key) {
    file.path(RDATA_DIR, key)
}

for (file in filelist) {
    cat(file, "\n")
    envir <- suppressPackageStartupMessages({source_env(file)})
    
}

